@article{baldwin_is_1999,
	title = {Is protein folding hierarchic? I. Local structure and peptide folding},
	volume = {24},
	issn = {0968-0004},
	shorttitle = {Is protein folding hierarchic?},
	abstract = {The folding reactions of some small proteins show clear evidence of a hierarchic process, whereas others, lacking detectable intermediates, do not. Nevertheless, we argue that both classes fold hierarchically and that folding begins locally. If this is the case, then the secondary structure of a protein is determined largely by local sequence information. Experimental data and theoretical considerations support this argument. Part I of this article reviews the relationship between secondary structures in proteins and their counterparts in peptides.},
