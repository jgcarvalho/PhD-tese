	keywords = {Computer Simulation, Protein Conformation, Molecular Sequence Data, Models, Molecular, Thermodynamics, Models, Chemical, Algorithms, Protein Folding, Peptide Fragments, Reproducibility of Results, Ribonucleases, Aprotinin}
}

