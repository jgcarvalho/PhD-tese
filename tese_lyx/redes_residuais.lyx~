\begin_layout Plain Layout
\begin_inset Caption Standard

\begin_layout Plain Layout
\begin_inset CommandInset label
LatexCommand label
name "tab:resnet-atribuicao"

\end_inset

Acurácia (
\begin_inset Formula $Q_{3}$
\end_inset

) das redes residuais em relação a estrutura secundária atribuída por diferentes
 métodos computacionais.
\end_layout

\end_inset


\end_layout

\end_inset


\end_layout

\begin_layout Subsection
Distribuição da acurácia
\end_layout

\begin_layout Standard
\begin_inset Note Note
status open

\begin_layout Plain Layout
grafico gerado com model_resnet_q_ss.py
\end_layout

\end_inset


\end_layout

